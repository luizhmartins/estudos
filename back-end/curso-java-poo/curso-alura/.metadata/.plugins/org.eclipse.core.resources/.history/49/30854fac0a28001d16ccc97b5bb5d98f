

	//import java.util.Scanner;
	
	public class EntradaDados {
	
		public static void main(String[] args) {
			
			String nome = "Luiz";
			int idade = 25;
			double peso = 58.256;
			char sexo = 'M';
			
			System.out.printf("%s", nome);
			System.out.printf("%d", idade);
			System.out.printf("%.2f", peso);
			System.out.printf("%s", sexo);
		}
	
	}
