

	import java.util.Scanner;
	
	public class EntradaDados {
	
		public static void main(String[] args) {
			
			String nome = "Luiz";
			int idade = 25;
			double peso = 58.256;
			char sexo = 'M';
			
			System.out.printf("%s%n", nome);
			System.out.printf("%i%n", idade);
			System.out.printf("%.2f%n", peso);
			System.out.printf("%c%n", sexo);
		}
	
	}
