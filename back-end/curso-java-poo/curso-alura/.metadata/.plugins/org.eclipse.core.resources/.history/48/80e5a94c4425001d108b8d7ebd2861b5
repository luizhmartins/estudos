
public class Gerente extends Funcionario {
	
	private int senha;
	
	public double getBonificacao() {
		return super.getBonificacao();
	}
	
	public void setSenha(int senha) {
		this.senha = senha;
	}
	
	private boolean autentica(int senha) {
		if(this.senha == senha) {
			return true;
		}else {
			return false;
		}
	}
}
